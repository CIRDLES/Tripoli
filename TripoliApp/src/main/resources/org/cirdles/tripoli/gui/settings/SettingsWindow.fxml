                    <font>
                        <Font name="Monospaced Bold" size="12.0"/>
                    </font>
                    <tooltip>
                        <Tooltip text="Save as defaults for this session">
                            <font>
                                <Font name="Monospaced Bold" size="12.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="26.0" stroke="TRANSPARENT"
                           strokeType="INSIDE" width="35.0"/>
                <Button fx:id="saveAsUserDefaultsButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="75.0"
                        text="User" textAlignment="CENTER" textFill="#a45151" wrapText="true">
                    <font>
                        <Font name="Monospaced Bold" size="12.0"/>
                    </font>
                    <tooltip>
                        <Tooltip text="Save as default settings for this user">
                            <font>
                                <Font name="Monospaced Bold" size="12.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="26.0" stroke="TRANSPARENT"
                           strokeType="INSIDE" width="9.0"/>
            </items>
        </ToolBar>
