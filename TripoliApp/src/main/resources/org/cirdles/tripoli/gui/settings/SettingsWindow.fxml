                <Label prefHeight="26.0" prefWidth="145.0" text="Save Current Settings:">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="26.0" layoutX="239.0" layoutY="15.0"
