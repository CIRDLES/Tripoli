                    <tooltip>
                        <Tooltip text="Restores settings to the user default settings.">
                            <font>
                                <Font name="Monospaced Bold" size="12.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
            </items>
