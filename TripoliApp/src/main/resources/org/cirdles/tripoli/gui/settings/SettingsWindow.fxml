                    <tooltip>
                        <Tooltip text="Restores settings to the session defaults.">
                            <font>
                                <Font name="Monospaced Bold" size="12.0"/>
                            </font>
                        </Tooltip>
                    </tooltip>
                </Button>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="26.0" stroke="TRANSPARENT"
